.index-swiper swiper{
  width: 100%;
  height: 340rpx;
}


.index-swiper swiper image{
  width: 100%;
}

.index-cate{
  display: flex;

}
.index-cate navigator{
  flex: 1;
  padding: 40rpx;
  text-align: center;
}

.index-cate navigator image{
  width: 100%;
}
.index-floor .floor-group .floor-title{
  padding: 10rpx 0;
}
.index-floor .floor-group .floor-title image{
  width: 200rpx;
}

.floor-list{
  /* 清除浮动，否则此组件没有高度值，与下面的view合为一个了 */
  overflow: hidden;
}

.floor-list navigator:nth-child(1){
  float: left;
  width: 33.33%;
  height: 400rpx;
}
.floor-list navigator:nth-child(n+2){
  float: left;
  width: 33.33%;
  height: 200rpx;
}

.floor-list navigator image{
  width: 100%;
  height: 100%;
  padding: 5rpx;
}